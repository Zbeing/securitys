<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper SYSTEM "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.zl.dao.IpMapper">
<insert id="add" parameterType="Iplog">
INSERT INTO t_Iplog(<include refid="colum"/>)
 VALUES(seq_log.nextval,#{ip},#{username},#{logintime},#{state})
</insert>
<!-- iplog 的各个字段名 -->
<sql id="colum">
ID,ip,username,logintime,state
</sql>
</mapper>